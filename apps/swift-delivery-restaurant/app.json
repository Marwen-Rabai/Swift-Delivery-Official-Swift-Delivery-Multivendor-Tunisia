{
  "expo": {
    "name": "Swift Delivery Restaurant",
    "slug": "swift-delivery-restaurant",
    "description": "Swift Delivery Restaurant is a modern restaurant management app built in React Native using Expo for IOS and Android. It provides restaurant owners with powerful tools to manage orders, menus, and customer interactions efficiently.",
    "version": "1.0.57",
    "orientation": "portrait",
    "icon": "./lib/assets/images/icon.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./lib/assets/images/black.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "plugins": [
      "expo-router",
      [
        "@sentry/react-native/expo",
        {
          "organization": "ninjas-code-w7",
          "project": "enatega-store-app",
          "url": "https://sentry.io/"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos for license, vehicle plate and profile image. Please allow these to continue using the app."
        }
      ],
      "expo-asset"
    ],
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.swift.delivery.restaurant",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "$(PRODUCT_NAME) uses your location for features like finding orders nearby and tracking customer orders!",
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "UIRequiresFullScreen": true
      }
    },
    "android": {
      "versionCode": 57,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "package": "com.swift.delivery.restaurant",
      "adaptiveIcon": {
        "foregroundImage": "./lib/assets/images/appIcon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "eas": {
        "projectId": "6a94161f-f21b-4ad9-8a04-c95b54ef0116"
      }
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
